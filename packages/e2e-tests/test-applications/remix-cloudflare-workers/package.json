  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "ts-node": "10.9.1",
    "typescript": "^5.1.6",
    "wrangler": "^3.1.1"
